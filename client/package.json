  },
  "type": "commonjs",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "bdevrs": "bun supabase stop --no-backup && bun i && bun supabase start && bun supabase db reset && next dev",
    "bdevx": "bun supabase stop && bun i && bun supabase start && next dev",
    "build": "next build",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "dev": "cross-env FORCE_COLOR=1 next dev",
    "devrs": "bunx supabase stop --no-backup && pnpm i && pnpm supabase start && pnpm supabase db reset && next dev",
    "devx": "bunx supabase stop && pnpm i && pnpm supabase start && next dev",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "bunx patch-package -y && bunx sort-package-json",
    "lint": "bunx biome check --write ./ || true && bunx stylelint src/styles/**/*.{css,scss,sass} --fix",
    "sb:diff": "bunx supabase db diff -f new_migration",
    "sb:link": "bunx supabase link",
    "sb:new": "bunx supabase migration new new_migration",
    "sb:pull": "bunx supabase db pull",
    "sb:push": "bunx supabase db push",
    "sb:reset": "bunx supabase db reset",
    "sb:seed": "bunx supabase db dump --data-only -f supabase/seed.sql",
    "sb:start": "bunx supabase start",
    "sb:status": "bunx supabase status",
    "sb:stop": "bunx supabase stop",
    "sb:typegen": "bunx supabase gen types typescript --local --schema public > src/types/supabase.ts",
    "sb:up": "bunx supabase migration up",
    "start": "next start",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:login": "stripe login",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "defaults",
      "not dead",
      "not IE 11",
      "not IE_Mob 11",
      "maintained node versions"
    ]
  },
